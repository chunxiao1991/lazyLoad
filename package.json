{
  "name": "yzttaro",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:h5:mock": "cross-env NODE_ENV=development TARO_APP_API=mock yarn dev:h5",
    "dev:h5:dev": "cross-env NODE_ENV=development TARO_APP_API=dev yarn dev:h5",
    "dev:h5:fat": "cross-env NODE_ENV=development TARO_APP_API=fat yarn dev:h5",
    "dev:h5:pre": "cross-env NODE_ENV=development TARO_APP_API=pre yarn dev:h5",
    "dev:h5:pro": "cross-env NODE_ENV=development TARO_APP_API=pro yarn dev:h5",
    "build:h5:dev": "cross-env NODE_ENV=production TARO_APP_API=dev yarn build:h5",
    "build:h5:fat": "cross-env NODE_ENV=production TARO_APP_API=fat yarn build:h5",
    "build:h5:pre": "cross-env NODE_ENV=production TARO_APP_API=pre yarn build:h5",
    "build:h5:pro": "cross-env NODE_ENV=production TARO_APP_API=pro yarn build:h5",
    "dev:weapp:mock": "cross-env NODE_ENV=development TARO_APP_API=mock yarn dev:weapp",
    "dev:weapp:dev": "cross-env NODE_ENV=development TARO_APP_API=dev yarn dev:weapp",
    "dev:weapp:fat": "cross-env NODE_ENV=development TARO_APP_API=fat yarn dev:weapp",
    "dev:weapp:pre": "cross-env NODE_ENV=development TARO_APP_API=pre yarn dev:weapp",
    "dev:weapp:pro": "cross-env NODE_ENV=development TARO_APP_API=pro yarn dev:weapp",
    "build:weapp:dev": "cross-env NODE_ENV=production TARO_APP_API=dev yarn build:weapp",
    "build:weapp:pre": "cross-env NODE_ENV=production TARO_APP_API=pre yarn build:weapp",
    "build:weapp:pro": "cross-env NODE_ENV=production TARO_APP_API=pro yarn build:weapp",
    "tep": "node template",
    "module": "node template-component",
    "gen:com": "cross-env TYPE=1  node template",
    "prettier": "prettier --write ./src/**/*.{js,jsx}",
    "lint:js": "eslint --ext .js src",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "mock": "gulp mock",
    "set:package:pro": "cross-env PACKAGE_ENV=production",
    "analyze:weapp": "cross-env ANALYZE=1 yarn build:weapp"
  },
  "author": "",
  "license": "MIT",
